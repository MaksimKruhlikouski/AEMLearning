<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:dam="http://www.day.com/dam/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:isCheckedOut="{Boolean}true"
    jcr:mixinTypes="[mix:referenceable,mix:versionable]"
    jcr:primaryType="dam:Asset">
    <jcr:content
        cq:name="simple-content-fragment"
        cq:parentPath="/content/dam/learning-project/content-fragments/us/en"
        jcr:primaryType="dam:AssetContent"
        jcr:title="Greetings Content Fragment"
        contentFragment="{Boolean}true">
        <data
            cq:model="/conf/learning-project/settings/dam/cfm/models/greetings-cf-model"
            jcr:primaryType="nt:unstructured"
            _strucVersion="{Long}1">
            <master
                jcr:mixinTypes="[cq:Taggable,dam:cfVariationNode]"
                jcr:primaryType="nt:unstructured"
                greetingsTitle="Greetings!"
                greetingsSubtitle="Hi there"/>
        </data>
        <metadata
            jcr:mixinTypes="[cq:Taggable]"
            jcr:primaryType="nt:unstructured"/>
        <related jcr:primaryType="nt:unstructured"/>
    </jcr:content>
</jcr:root>
