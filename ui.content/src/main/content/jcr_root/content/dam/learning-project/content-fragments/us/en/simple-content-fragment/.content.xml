<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:dam="http://www.day.com/dam/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:mix="http://www.jcp.org/jcr/mix/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:isCheckedOut="{Boolean}true"
    jcr:mixinTypes="[mix:referenceable,mix:versionable]"
    jcr:primaryType="dam:Asset">
    <jcr:content
        cq:name="simple-content-fragment"
        cq:parentPath="/content/dam/learning-project/content-fragments/us/en"
        jcr:primaryType="dam:AssetContent"
        jcr:title="Simple Content Fragment"
        contentFragment="{Boolean}true">
        <data
            cq:model="/conf/learning-project/settings/dam/cfm/models/simple-cf-model"
            jcr:primaryType="nt:unstructured"
            _strucVersion="{Long}1">
            <master
                jcr:mixinTypes="[cq:Taggable,dam:cfVariationNode]"
                jcr:primaryType="nt:unstructured"
                authorBio="&lt;p>Some Description&lt;/p>&#xa;"
                authorBio_x0040_ContentType="text/html"
                authorName="Joanne Rowling"
                books="[Harry Potter 1,Harry Potter 2]"
                isAuthor="{Boolean}true"
                profession="author"
                publishDate="{Date}2025-05-15T00:00:00.000Z"/>
            <author
                jcr:mixinTypes="[cq:Taggable,dam:cfVariationNode]"
                jcr:primaryType="nt:unstructured"
                authorBio="&lt;p>Some Description&lt;/p>&#xa;"
                authorBio_x0040_ContentType="text/html"
                authorName="Joanne Rowling Author 2"
                books="[Harry Potter 1,Harry Potter 2,Harry Potter 3]"
                isAuthor="{Boolean}true"
                profession="both"
                publishDate="{Date}2025-05-25T00:00:00.000Z"/>
        </data>
        <metadata
            jcr:mixinTypes="[cq:Taggable]"
            jcr:primaryType="nt:unstructured"/>
        <model jcr:primaryType="nt:unstructured">
            <variations jcr:primaryType="nt:unstructured">
                <author
                    jcr:description=""
                    jcr:mixinTypes="[cq:Taggable]"
                    jcr:primaryType="nt:unstructured"
                    jcr:title="Author 2"
                    name="author">
                    <properties jcr:primaryType="nt:unstructured"/>
                </author>
            </variations>
        </model>
        <related jcr:primaryType="nt:unstructured"/>
    </jcr:content>
</jcr:root>
